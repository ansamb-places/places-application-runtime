<div class="conversation-div">
<div class='dropOverlay'>
		<div class='notif'>Drop it!</div>
</div>
<section class="module" id="<%- placeName %>">
	<header class="top-bar"> 
		<div class="left">
			<!-- <span class="online-left"></span> -->
			<!-- <span class="entypo">
			&#59160;
			</span> -->
			<span class="ansambers">
			  <% _.each(ansambers,function(item,index){ %>
			    <span class='ansamber-name'><%-item.firstname%> <%-item.lastname%></span>
			  <% }) %>
			</span>
		</div>
		<div class="right">
			<a class="p-ci add-contact"><span class="fi-plus"></span></a>
			<a class="p-ci visio-contact"><span class="fi-video"></span></a>		
			<a class="p-ci audio-contact"><span class="fi-telephone"></span></a>
			<a class="p-ci close-chat"><span class="fi-x"></span></a>
		</div>
	</header>
	<div class="conversation ">
		<ol class="discussion">
		</ol>
		<div class="reply">
			<textarea id="new_post" type="text" placeholder="Add A Reply"/>
		</div>
	</div>
</section>
</div>

