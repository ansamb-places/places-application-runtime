<div class="activityFeedContainer p-account-info selectable">
  <div> <span> first name : </span> <%- firstname %>  </div><br/>
  <div> <span> last name : </span> <%- lastname %>  </div><br/>
  <div> <span> aliases : </span>
  	<ul class="p-aliases-list">
	  	<% _.each(aliases,function(alias,key){%>
  			<li><span class="label"><%-alias.type%></span><span><%- alias.alias %></span> </li>
  		<%})%>
    </ul>
  </div>
</div>